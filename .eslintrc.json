{
  "extends": [
    "react-app",
    "airbnb"
  ],
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "flowtype"
  ],
  "rules": {
    "comma-dangle": [
      2,
      "never"
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/no-unescaped-entities": [
      0
    ],
    "react/prop-types": 0,
    "jsx-a11y/href-no-hash": 0,
    "jsx-a11y/img-has-alt": 0,
    "no-console": 0,
    "max-len": 0
  }
}
