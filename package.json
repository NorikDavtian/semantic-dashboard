{
  "name": "semantic-dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "history": "^4.7.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.72.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "eslint": "^4.7.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-react-app": "^2.0.0",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "husky": "^0.14.3",
    "lint-staged": "^3.6.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "react-scripts": "0.9.5"
  },
  "scripts": {
    "build": "npm run minify-css && react-scripts build",
    "build-css": "node-sass src/ -o src/ --output-style expanded",
    "clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install",
    "eject": "react-scripts eject",
    "fixit": "eslint src/ --fix",
    "lint": "eslint src/",
    "minify-css": "node-sass src/ -o src/ --output-style compressed",
    "precommit": "lint-staged",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --output-style expanded --watch --recursive"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
